{
  "name": "devsummit",
  "version": "2019.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && rollup -c && del .build-tmp",
    "watch": "npm run clean && rollup -c --watch",
    "clean": "del build .build-tmp",
    "deploy:prod": "npm run build && firebase deploy",
    "serve:netlify": "netlify dev -c 'npm run build' -d 'build'",
    "serve:firebase": "HOST='http://localhost:5000' firebase serve --only hosting,functions",
    "loadsecrets": "firebase functions:config:get > functions/.runtimeconfig.json"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@types/joi": "^14.3.3",
    "cssnano": "^4.1.10",
    "date-and-time": "^0.8.1",
    "del-cli": "^2.0.0",
    "escape-string-regexp": "^2.0.0",
    "express": "^4.17.1",
    "find-free-port": "^2.0.0",
    "firebase": "^6.3.1",
    "firebase-tools": "^7.1.0",
    "glob": "^7.1.4",
    "html-minifier": "^4.0.0",
    "husky": "^3.0.0",
    "idb-keyval": "^3.2.0",
    "lit-html": "^1.1.1",
    "mkdirp": "^0.5.1",
    "postcss": "^7.0.17",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-modules": "^1.4.1",
    "postcss-nested": "^4.1.2",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "require-from-string": "^2.0.2",
    "rollup": "^1.17.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {}
}
